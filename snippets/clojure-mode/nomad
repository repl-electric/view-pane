#name: nomad
# --
(do(def nomad-chord-g (chord-synth general-purpose-assembly 4 :amp 0.00 :saw-cutoff 2000 :wave 0 :attack 1.0 :release 5.0 :noise-level 0.05 :beat-trg-bus (:beat time/beat-1th) :beat-bus (:count time/beat-1th)))(def nomad-start (first (:bufs nomad-chord-g))))$1

(ctl nomad-chord-g :amp 0.1)
(pattern! nomad-start (repeat 1 (degrees-seq [$0])))
