#name: soprano-yeh
# --
(do
(defonce singers-g (group "singers"))
(defonce note-buf (buffer 128))
(defonce seq-b4 (buffer 128))
(doseq [i (range 0 3)]
  (slow-singer
   [:head singers-g]
   :note-buf note-buf :amp 2.9
   :beat-b (:beat time/beat-4th) :count-b (:count time/beat-4th)
   :seq-b seq-b4
   :beat-num i
   :index-b yeh-index-buffer
   :num-steps 3)))
