#name: peek-inside
# --
(do ;;Lets mess with running buffer
  (chord-pattern!
   nomad-chord-g
   (map
    (fn [thing-like]
      (cond
       (some #{thing-like} (degrees [$0] :minor :f4)) (degrees [7] :minor :f3)
       ;;(some #{thing-like} (degrees [3] :minor :f4)) (degrees [4] :minor :f3)
       ;;(some #{thing-like} (degrees [7] :minor :f3)) (degrees [1] :minor :f3)
       :else
       [thing-like]))
    (map int (buffer-data (first (:bufs nomad-chord-g)))))))
