#name: flt-beat
# --
(do
  (defonce df-b (buffer 256))
  (pattern! df-b [(degrees [1 1 1 1] :minor :F0)(degrees [1 1 1 1] :minor :F1)(degrees [1 1 1 1] :minor :F2)(degrees [1 1 1 1] :minor :F1)])
  (pattern! kick-seq-buf (repeat 3
                                 [1 0 0 0 1 0 0 0   1 0 0 0 1 0 0 0])
                                 [1 0 0 0 1 0 0 0   1 0 0 0 1 0 1 0])
  ;;(def f (dulcet-fizzle :amp 2.0 :note-buf df-b))
)$0
